{
  "name": "mongo-bongo",
  "version": "1.0.2",
  "description": "A tool for easy MongoDB database configuration and backup",
  "bin": {
    "bongo": "dist/bongo.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "babel src -d dist -s --ignore *.test.js",
    "debug:snap": "babel-node --inspect-brk src/bongo.js",
    "test": "jest",
    "test:debug": "babel-node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules/",
      "scratch/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlyonsmith/mongo-bongo.git"
  },
  "keywords": [
    "mongo",
    "database",
    "authentication",
    "backup",
    "restore"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/mongo-bongo/issues"
  },
  "homepage": "https://github.com/jlyonsmith/mongo-bongo#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "chalk": "^2.2.0",
    "command-exists": "^1.2.2",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "js-yaml": "^3.11.0",
    "json5": "^1.0.1",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "randomatic": "^3.0.0",
    "snap-tool": "^1.4.0",
    "tmp-promise": "^1.0.4"
  }
}
